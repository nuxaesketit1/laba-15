#include <iostream>
#include <cmath>
using namespace std;

int main() {

	setlocale(0, "");

	int n;
	double temp;

	do {

		cout << "Введите количество элементов массива N (N > 0) : ";
		cin >> n;
		cout << endl;

	} while (n<0);

	double* array = new double[n];
	double* array_1 = new double[n];

	cout << "Введите элементы 1-го массива : ";

	for (int i = 0; i < n; i++) {

		cin >> array[i];

	}

	cout << "Введите элементы 2-го массива : ";

	for (int i = 0; i < n; i++) {

		cin >> array_1[i];

	}

	cout << "Результат :\nЭлементы 1-го массива : " << endl;

	for (int i = 0; i < n; i++) {

		temp = array[i];
		array[i] = array_1[i];
		array_1[i] = temp;
		cout << array[i] << " ";

	}

	cout << endl;
	cout << "Элементы 2-го массива : " << endl;

	for (int i = 0; i < n; i++) {

		cout << array_1[i] << " ";
	
	}

	cout << endl;

	system("pause");

	return 0;

}
